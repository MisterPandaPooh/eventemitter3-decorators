{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": ["google", "prettier"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint", "prettier"],
    "rules": {
        "no-console": 0,
        "prettier/prettier": 2
    }
}
